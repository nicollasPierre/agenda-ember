<style>
.demo-card-square.mdl-card {
  width: 512px;
  height: 320px;
}
.demo-card-square > .mdl-card__title {
  color: #fff;
  background:
    url({{model.foto}}) bottom right 15% no-repeat #46B6AC;
  background-size: 185px 185px;
}
</style>


<div align="center">
  <div class="demo-card-square mdl-card mdl-shadow--2dp" >
    <div class="mdl-card__title mdl-card--expand">
      <h2 class="mdl-card__title-text">{{model.nome}}</h2>
    </div>
    <div class="mdl-card__supporting-text">
      <b>Email:</b> {{model.email}}
      <b>Data Nascimento:</b> {{model.dtNasc}}
      <b>Tipo de telefone:</b> {{model.tipoTelefone}}
      <b>Tipo pessoa:</b> {{model.tipoPessoa}}
      <b>Numero telefone:</b> {{model.celular}}

    </div>
    <div class="mdl-card__actions mdl-card--border">
      <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
        Editar
      </a>
      <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
        Remover
      </a>
    </div>
  </div>

  {{outlet}}
</div>
